{
  "name": "cohart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios  --scheme  \"cohart (beta)\"",
    "ios:production": "ENVFILE=.env.production react-native run-ios --scheme  \"cohart (prod)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "tslint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "postinstall": "patch-package",
    "update:pods": "npx pod-install ios",
    "release": "react-native run-android --variant=release",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "setup:ios": "cd ios && pod install && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "android:production-release": "ENVFILE=.env.production react-native run-android --variant=release",
    "android:staging-release": "ENVFILE=.env.staging  react-native run-android --variant=release",
    "android-production-bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "android-staging-bundle": "cd android && ENVFILE=.env.staging ./gradlew bundleRelease",
    "copy-dev-firebaseFile": "cp -R  android/app/google-services-dev.json android/app/google-services.json ",
    "copy-prd-firebaseFile": "cp -R  android/app/google-services-prod.json  android/app/google-services.json",
    "copy-ios-firebase-dev-file": "cp ios/Firebase/Dev/GoogleService-Info-dev.plist  ios/GoogleService-Info.plist",
    "copy-ios-firebase-prd-file": "cp ios/Firebase/Prod/GoogleService-Info-prod.plist  ios/GoogleService-Info.plist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "6.0.2",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-firebase/analytics": "^13.0.1",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/crashlytics": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-native-seoul/masonry-list": "^1.1.1",
    "@react-navigation/bottom-tabs": "6.0.5",
    "@react-navigation/drawer": "6.1.4",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/native-stack": "6.1.0",
    "@redux-saga/testing-utils": "^1.1.3",
    "@segment/analytics-react-native": "1.5.0",
    "@skele/components": "1.0.0-alpha.40",
    "appcenter": "4.2.0",
    "appcenter-analytics": "4.2.0",
    "appcenter-crashes": "4.2.0",
    "axios": "0.21.4",
    "google-libphonenumber": "3.2.22",
    "husky": ">=6",
    "isomorphic-ws": "4.0.1",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "react": "17.0.1",
    "react-hook-form": "^7.17.2",
    "react-native": "0.64.2",
    "react-native-actions-sheet": "0.5.4",
    "react-native-branch": "5.2.1",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-code-push": "7.0.2",
    "react-native-collapsible": "^1.6.0",
    "react-native-config": "1.4.4",
    "react-native-country-picker-modal": "2.0.0",
    "react-native-create-thumbnail": "1.4.1",
    "react-native-cross-actionsheet": "1.0.1",
    "react-native-device-info": "8.3.3",
    "react-native-dropdown-picker": "5.1.27",
    "react-native-exit-app": "1.1.0",
    "react-native-fast-image": "8.5.8",
    "react-native-fast-video": "5.1.7",
    "react-native-flash-message": "0.2.0",
    "react-native-fs": "2.18.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-gifted-chat": "0.16.3",
    "react-native-image-crop-picker": "0.36.2",
    "react-native-jw-media-player": "0.1.57",
    "react-native-keyboard-aware-scroll-view": "0.9.4",
    "react-native-linear-gradient": "2.5.6",
    "react-native-modal": "11.7.0",
    "react-native-paper": "4.9.2",
    "react-native-permissions": "3.0.5",
    "react-native-phone-number-input": "2.1.0",
    "react-native-popup-menu": "^0.15.11",
    "react-native-push-notification-popup": "1.6.0",
    "react-native-reanimated": "2.2.2",
    "react-native-render-html": "5.1.1",
    "react-native-responsive-screen": "1.4.2",
    "react-native-restart": "0.0.22",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.7.2",
    "react-native-share": "7.1.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "8.1.0",
    "react-native-version-check": "^3.4.2",
    "react-redux": "7.2.5",
    "reanimated-bottom-sheet": "1.0.0-alpha.22",
    "redux": "4.1.1",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "rn-fetch-blob": "0.12.0",
    "rn-qr-generator": "^1.2.0",
    "smartlook-react-native-wrapper": "1.2.1",
    "styled-components": "5.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.5",
    "@babel/runtime": "7.15.3",
    "@react-native-community/eslint-config": "2.0.0",
    "@redux-saga/testing-utils": "^1.1.3",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "8.0.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "^4.14.176",
    "@types/react-native": "0.64.13",
    "@types/react-native-restart": "0.0.14",
    "@types/react-native-vector-icons": "6.4.8",
    "@types/react-native-version-check": "^3.4.3",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "eslint": "7.32.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "3.9.10"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "collectCoverage": true,
    "verbose": true,
    "testEnvironment": "jsdom"
  },
  "lint-staged": {
    "*.{json,md,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": "eslint . --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
